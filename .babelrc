{
  "presets": ["next/babel"],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "preprocess": false,
        "displayName": true,
        "fileName": true,
        "pure": true
      }
    ]
  ]
}
